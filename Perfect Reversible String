class Solution
{
    public int isReversible(String str, int n) 
    { 
    
        int i = 0, j = str.length() - 1;
 
        while (i < j) {
 
            
            if (str.charAt(i) != str.charAt(j))
                return 0;
 
            i++;
            j--;
        }
 
        
        return 1;
    } 
}
