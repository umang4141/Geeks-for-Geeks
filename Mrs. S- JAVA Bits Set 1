class GfG{
    public void count(long n){
        // your code here.
        long c1 = 0;
        long c2 = 0;
        while(n>0)
        {
            long rem = n % 2;
            if(rem == 1)
            {
                c1++;
            }
            else
            {
                c2++;
            }
            n = n / 2;
        }
        System.out.println(c1 + " " + c2);
    }
}
